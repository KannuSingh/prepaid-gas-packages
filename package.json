{
  "name": "prepaid-gas-packages",
  "version": "0.0.1",
  "private": true,
  "description": "Prepaid Gas packages monorepo",
  "license": "MIT",
  "author": "Karandeep Singh",
  "homepage": "https://github.com/KannuSingh/prepaid-gas-packages#readme",
  "bugs": {
    "url": "https://github.com/KannuSingh/prepaid-gas-packages/issues"
  },
  "keywords": [
    "account-abstraction",
    "erc-4337",
    "paymaster",
    "zero-knowledge",
    "semaphore",
    "privacy",
    "ethereum",
    "gas-payments",
    "zk-proofs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/prepaid-gas-packages.git"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "clean": "turbo clean",
    "clean:node-modules": "rm -rf node_modules && rm -rf apps/*/node_modules && rm -rf packages/*/node_modules && rm -rf tools/*/node_modules",
    "clean:all": "pnpm run clean && pnpm run clean:node-modules",
    "analyze": "turbo analyze",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.17.0",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "pnpm": {
    "overrides": {
      "@noble/curves": "1.2.0"
    },
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "bufferutil",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "utf-8-validate"
    ]
  }
}