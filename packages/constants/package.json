{
  "name": "@prepaid-gas/constants",
  "version": "0.0.5",
  "description": "Shared constants for Prepaid Gas packages",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist",
    "analyze": "rollup -c rollup.config.mjs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "constants",
    "paymaster",
    "account-abstraction",
    "erc-4337",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KannuSingh/prepaid-gas-packages.git",
    "directory": "packages/constants"
  },
  "license": "MIT",
  "peerDependencies": {
    "viem": "^2.31.3"
  },
  "devDependencies": {
    "@prepaid-gas/eslint-config": "workspace:*",
    "@prepaid-gas/typescript-config": "workspace:*",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "eslint": "^9.17.0",
    "rollup": "^4.28.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "viem": "^2.31.3",
    "vitest": "^2.1.8"
  }
}