{
  "name": "@private-prepaid-gas/types",
  "version": "1.0.0",
  "description": "TypeScript types and interfaces for Private Prepaid Gas paymaster system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "typescript",
    "types",
    "paymaster",
    "account-abstraction",
    "erc-4337",
    "subgraph"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/private-prepaid-gas-packages.git",
    "directory": "packages/types"
  },
  "license": "MIT",
  "dependencies": {
    "@private-prepaid-gas/contracts": "workspace:*"
  },
  "devDependencies": {
    "@private-prepaid-gas/eslint-config": "workspace:*",
    "@private-prepaid-gas/typescript-config": "workspace:*",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}