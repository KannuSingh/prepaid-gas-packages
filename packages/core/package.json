{
  "name": "@prepaid-gas/core",
  "version": "0.0.5",
  "description": "Core SDK for Prepaid Gas paymaster integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist rollup-dist",
    "analyze": "rollup -c rollup.analyze.mjs && open rollup-dist/bundle-analysis.html"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "paymaster",
    "account-abstraction",
    "erc-4337",
    "zero-knowledge",
    "semaphore",
    "privacy",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KannuSingh/prepaid-gas-packages.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "dependencies": {
    "@prepaid-gas/constants": "workspace:*",
    "@prepaid-gas/data": "workspace:*",
    "@semaphore-protocol/group": "^4.11.1",
    "@semaphore-protocol/identity": "^4.11.1",
    "@semaphore-protocol/proof": "^4.11.1"
  },
  "peerDependencies": {
    "viem": "^2.28.1"
  },
  "devDependencies": {
    "@prepaid-gas/eslint-config": "workspace:*",
    "@prepaid-gas/typescript-config": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.17.0",
    "rollup": "^4.46.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "viem": "^2.28.1",
    "vitest": "^2.1.8"
  }
}