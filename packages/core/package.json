{
  "name": "@private-prepaid-gas/core",
  "version": "1.0.0",
  "description": "Core SDK for Private Prepaid Gas paymaster integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "paymaster",
    "account-abstraction",
    "erc-4337",
    "zero-knowledge",
    "semaphore",
    "privacy",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/private-prepaid-gas-packages.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "dependencies": {
    "@private-prepaid-gas/contracts": "workspace:*",
    "@private-prepaid-gas/types": "workspace:*",
    "@private-prepaid-gas/data": "workspace:*",
    "@semaphore-protocol/core": "^4.11.1",
    "@semaphore-protocol/group": "^4.11.1",
    "@semaphore-protocol/identity": "^4.11.1",
    "@semaphore-protocol/proof": "^4.11.1",
    "poseidon-lite": "^0.3.0",
    "viem": "^2.31.3"
  },
  "devDependencies": {
    "@private-prepaid-gas/eslint-config": "workspace:*",
    "@private-prepaid-gas/typescript-config": "workspace:*",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
