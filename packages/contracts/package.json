{
  "name": "@private-prepaid-gas/contracts",
  "version": "1.0.0",
  "description": "Smart contract ABIs and deployment addresses for Private Prepaid Gas paymasters",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ethereum",
    "account-abstraction",
    "paymaster",
    "contracts",
    "abi",
    "erc-4337"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/private-prepaid-gas-packages.git",
    "directory": "packages/contracts"
  },
  "license": "MIT",
  "devDependencies": {
    "@private-prepaid-gas/eslint-config": "workspace:*",
    "@private-prepaid-gas/typescript-config": "workspace:*",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}